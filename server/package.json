{
  "name": "obplus-server",
  "version": "3.2.0",
  "author": "Paramate Tawilapakul",
  "main": "./src/server.js",
  "scripts": {
    "start": "set NODE_ENV=production&& node ../build/dist/index.js",
    "start_dev": "set NODE_ENV=development&& nodemon ./src/server.js",
    "dev": "concurrently \"npm run start_dev\" \"npm run client\"",
    "lint": "eslint .",
    "client": "npm run dev --prefix ../client",
    "clientbuild": "npm run build --prefix ../client",
    "build2": "ncc build ./src/server.js -m -o ../build/dist && npm run clientbuild",
    "build": "ncc build ./src/server.js -m -o ../build/dist ",
    "build_prod": "ncc build ./src/server.js -m -o C:/codespace/obplus/build/1.obplus_fastify/dist && npm run clientbuild"
  },
  "dependencies": {
    "@fastify/compress": "^8.0.3",
    "@fastify/cookie": "^7.4.0",
    "@fastify/cors": "^8.5.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/jwt": "^8.0.1",
    "@fastify/multipart": "^8.3.0",
    "@fastify/one-line-logger": "^2.0.2",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/static": "^7.0.4",
    "axios": "^1.9.0",
    "base64-img": "^1.0.4",
    "bcrypt": "^5.1.1",
    "cacheable": "^1.10.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "fastify": "^4.29.1",
    "graceful-fs": "^4.2.11",
    "jsonwebtoken": "^8.5.1",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "mssql": "^11.0.1",
    "node-schedule": "^2.1.1",
    "pino-pretty": "^9.4.1",
    "sharp": "^0.30.7",
    "ua-parser-js": "^2.0.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.27.0",
    "globals": "^16.2.0"
  }
}
